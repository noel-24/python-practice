 for attempt in range(max_retries):\n","        try:\n","            return callable_func().execute()\n","        except HttpError as e:\n","            status = getattr(e.resp, \"status\", None)\n","            text = str(e)\n","            # quotaExceeded\n","            if status == 403 and \"quotaExceeded\" in text:\n","                wait = min(base_wait * (2 ** attempt), 60)\n","                print(f\"[API] quotaExceeded -> {wait}s 待機して再試行 ({attempt+1}/{max_retries})\")\n","                time.sleep(wait)\n","                continue\n","            # 5xx server errors\n","            if status and 500 <= status < 600:\n","                wait = min(base_wait * (2 ** attempt), 60)\n","                print(f\"[API] server error {status} -> {wait}s 待機して再試行 ({attempt+1}/{max_retries})\")\n","                time.sleep(wait)\n","                continue\n","            raise\n","    raise RuntimeError(\"API リトライ上限に達しました\")\n","\n","# -------------------------------\n","# ユーティリティ：JST <-> UTC ISO 変換等\n","# -------------------------------\n","def sheet_datetime_to_utc_iso(date_str, time_str):\n","    # sheet の date: \"YYYY/MM/DD\", time: \"HH:MM\"（time が空なら 00:00）\n","    if not date_str:\n","        return None\n","    if not time_str:\n","        time_str = \"00:00\"\n","    dt_jst = datetime.strptime(f\"{date_str} {time_str}\", \"%Y/%m/%d %H:%M\")\n","    # dt_jst is naive JST; convert to UTC\n","    dt_utc = dt_jst - timedelta(hours=9)\n","    return dt_utc.strftime(\"%Y-%m-%dT%H:%M:%SZ\")\n","\n","def iso_to_jst_datetime(dt_iso):\n","    if not dt_iso:\n","        return None\n","    try:\n","        dt = isodate.parse_datetime(dt_iso)\n","    except Exception:\n","        try:\n","            dt = datetime.strptime(dt_iso, \"%Y-%m-%dT%H:%M:%SZ\").replace(tzinfo=timezone.utc)\n","        except:\n","            return None\n","    # normalize to UTC\n","    if dt.tzinfo is None:\n","        dt = dt.replace(tzinfo=timezone.utc)\n","    dt_utc = dt.astimezone(timezone.utc)\n","    dt_jst = dt_utc + timedelta(hours=9)\n","    # return naive JST datetime\n","    return dt_jst.replace(tzinfo=None)\n","\n","def jst_datetime_to_date_time_strs(dt_jst):\n","    if dt_jst is None:\n","        return \"\", \"\"\n","    return dt_jst.strftime(\"%Y/%m/%d\"), dt_jst.strftime(\"%H:%M\")\n","\n","# -------------------------------\n","# (A) マスターから channel ID リストを取得\n","# -------------------------------\n","def get_id_list():\n","    sh = gc.open_by_key(CHID_SPREADSHEET_KEY)\n","    ws = sh.worksheet(\"chID\")\n","    vals = ws.col_values(2)  # B列に channel IDs\n","    return [v.strip() for v in vals if v.strip()]\n","\n","# -------------------------------\n","# (B) チャンネルの uploads プレイリストID と チャンネル名取得\n","# -------------------------------\n","def get_uploads_playlist_id(channel_id):\n","    res = api_call_with_retry(lambda: youtube.channels().list(part=\"contentDetails,snippet\", id=channel_id))\n","    items = res.get(\"items\", [])\n","    if not items:\n","        raise RuntimeError(f\"channel not found: {channel_id}\")\n","    item = items[0]\n","    title = item[\"snippet\"][\"title\"]\n","    uploads = item[\"contentDetails\"][\"relatedPlaylists\"][\"uploads\"]\n","    return uploads, title\n","\n","# -------------------------------\n","# (C) 既存スプシから最後に取得した日時を判定（JST datetime or None）\n","# 説明: 各シートの「日付」「開始時間」列をチェックし、最大日時を返す\n","# -------------------------------\n","def get_last_retrieved_datetime_for_sheet(sh):\n","    latest = None\n","    for ws in sh.worksheets():\n","        # try to get header row to map columns\n","        try:\n","            records = ws.get_all_records()\n","        except Exception:\n","            continue\n","        if not records:\n","            continue\n","        # records is list of dicts keyed by header strings\n","        # expect headers \"日付\" and \"開始時間\" (case sensitive)\n","        for r in records:\n","            date_str = r.get(\"日付\") or r.get(\"日付\".encode('utf-8'), \"\")\n","            time_str = r.get(\"開始時間\") or r.get(\"開始時間\".encode('utf-8'), \"\")\n","            if not date_str:\n","                continue\n","            utc_iso = sheet_datetime_to_utc_iso(date_str, time_str)\n","            if not utc_iso:\n","                continue\n","            # convert to JST datetime for comparison\n","            dt_jst = iso_to_jst_datetime(utc_iso)\n","            if not dt_jst:\n","                continue\n","            if latest is None or dt_jst > latest:\n","                latest = dt_jst\n","    return latest  # naive JST datetime or None\n","\n","# -------------------------------\n","# (D) playlistItems を先頭から走査し、since_utc_iso より新しい videoId を収集\n","#  - since_utc_iso: ISO UTC string \"YYYY-MM-DDTHH:MM:SSZ\" （exclusive）\n","#  - 戻り: list of videoIds ordered newest→older\n","# -------------------------------\n","def get_video_ids_since(playlist_id, since_utc_iso):\n","    ids = []\n","    next_page = None\n","    while True:\n","        pl = api_call_with_retry(lambda: youtube.playlistItems().list(\n","            part=\"contentDetails\",\n","            playlistId=playlist_id,\n","            maxResults=50,\n","            pageToken=next_page\n","        ))\n","        stop_early = False\n","        for item in pl.get(\"items\", []):\n","            pub = item[\"contentDetails\"].get(\"videoPublishedAt\")\n","            vid = item[\"contentDetails\"].get(\"videoId\")\n","            if not pub or not vid:\n","                continue\n","            # pub is ISO UTC string\n","            if since_utc_iso:\n","                # if published <= since then we've reached already retrieved area -> stop\n","                if pub <= since_utc_iso:\n","                    stop_early = True\n","                    break\n","            # collect newer videos (pub > since) or all if since none\n","            ids.append(vid)\n","        if stop_early:\n","            break\n","        next_page = pl.get(\"nextPageToken\")\n","        if not next_page:\n","            break\n","    return ids\n","\n","# -------------------------------\n","# (E) videos.list で詳細を取得し DataFrame を返す\n","# -------------------------------\n","\n","# 動画時間の秒数を H:MM:SS 形式の文字列に変換\n","def format_duration(seconds):\n","    m, s = divmod(int(seconds), 60)\n","    h, m = divmod(m, 60)\n","    return f\"{h}:{m:02}:{s:02}\"\n","\n","def get_video_details(video_ids):\n","    # 列定義を変更\n","    columns = [\"日付\", \"開始時間\", \"終了時間\", \"動画時間\", \"タイトル\", \"種別\", \"URL\"]\n","\n","    if not video_ids:\n","        return pd.DataFrame(columns=columns)\n","\n","    rows = []\n","    for i in range(0, len(video_ids), 50):\n","        chunk = video_ids[i:i+50]\n","        res = api_call_with_retry(lambda: youtube.videos().list(\n","            part=\"snippet,contentDetails,liveStreamingDetails\",\n","            id=\",\".join(chunk)\n","        ))\n","        for item in res.get(\"items\", []):\n","            sn = item.get(\"snippet\", {})\n","            cd = item.get(\"contentDetails\", {})\n","\n","            # determine start/end time priority: actualStart -> scheduledStart -> publishedAt\n","            ld = item.get(\"liveStreamingDetails\", {}) or {}\n","            started_iso = ld.get(\"actualStartTime\") or ld.get(\"scheduledStartTime\") or sn.get(\"publishedAt\")\n","            ended_iso = ld.get(\"actualEndTime\") or ld.get(\"scheduledEndTime\")\n","\n","            jst_started = iso_to_jst_datetime(started_iso) if started_iso else None\n","            jst_ended = iso_to_jst_datetime(ended_iso) if ended_iso else None\n","\n","            date_str, start_time = jst_datetime_to_date_time_strs(jst_started)\n","            end_time = jst_ended.strftime(\"%H:%M\") if jst_ended else \"\"\n","\n","            # 動画時間の取得と整形\n","            dur_iso = cd.get(\"duration\", \"PT0S\")\n","            try:\n","                sec = isodate.parse_duration(dur_iso).total_seconds()\n","                duration_str = format_duration(sec)\n","            except Exception:\n","                sec = 0\n","                duration_str = \"\"\n","\n","            # classify\n","            kind = \"通常\"\n","            if ld:\n","                kind = \"ライブ\"\n","            elif sn.get(\"liveBroadcastContent\") in (\"live\", \"upcoming\"):\n","                kind = \"ライブ\"\n","            else:\n","                if sec > 0 and sec < SHORT_SECONDS:\n","                    kind = \"ショート\"\n","\n","            rows.append({\n","                \"日付\": date_str,\n","                \"開始時間\": start_time,\n","                \"終了時間\": end_time,\n","                \"動画時間\": duration_str,\n","                \"タイトル\": sn.get(\"title\", \"\"),\n","                \"種別\": kind,\n","                \"URL\": f\"https://www.youtube.com/watch?v={item.get('id')}\"\n","            })\n","\n","    if not rows:\n","        return pd.DataFrame(columns=columns)\n","\n","    df = pd.DataFrame(rows)\n","    # 時間順にソート\n","    df[\"__jst_dt\"] = df.apply(lambda r: datetime.strptime(f\"{r['日付']} {r['開始時間'] or '00:00'}\", \"%Y/%m/%d %H:%M\"), axis=1)\n","    df = df.sort_values(\"__jst_dt\").drop(columns=\"__jst_dt\").reset_index(drop=True)\n","\n","    # 列順序を保証して返す\n","    return df[columns]\n","\n","# -------------------------------\n","# (F) スプレッドシート操作：既存データを読み込み、月ごとにマージして上書き\n","# -------------------------------\n","def append_and_merge_to_spreadsheet(sh, df_new):\n","    if df_new.empty:\n","        return\n","\n","    # group by sheet name (YYYY/MM) based on 日付 column\n","    df_new[\"sheet\"] = df_new[\"日付\"].apply(lambda d: d.replace(\"/\", \"-\"))  # temp; we'll format later\n","    # create mapping sheet_name -> rows\n","    grouped = {}\n","    for _, row in df_new.iterrows():\n","        # transform 日付 \"YYYY/MM/DD\" -> sheet \"YYYY/MM\"\n","        ymd = row[\"日付\"]\n","        if not ymd:\n","            continue\n","        y, m, _ = ymd.split(\"/\")\n","        sheet_name = f\"{y}/{int(m):02d}\"\n","        grouped.setdefault(sheet_name, []).append({\n","            \"日付\": row[\"日付\"],\n","            \"開始時間\": row[\"開始時間\"],\n","            \"終了時間\": row[\"終了時間\"],\n","            \"動画時間\": row[\"動画時間\"],\n","            \"タイトル\": row[\"タイトル\"],\n","            \"種別\": row[\"種別\"],\n","            \"URL\": row[\"URL\"]\n","        })\n","\n","    # for each sheet, read existing, concat, dedupe by URL, sort, write back\n","    for sheet_name, rows in grouped.items():\n","        new_df = pd.DataFrame(rows)\n","        try:\n","            ws = sh.worksheet(sheet_name)\n","            try:\n","                existing = pd.DataFrame(ws.get_all_records())\n","            except Exception:\n","                existing = pd.DataFrame()\n","            if existing.empty:\n","                merged = new_df\n","            else:\n","               if \"動画時間\" not in existing.columns:\n","                  existing[\"動画時間\"] = \"\"\n","\n","               merged = pd.concat([existing, new_df], ignore_index=True)\n","               merged.drop_duplicates(subset=[\"URL\"], inplace=True)\n","\n","               # sort by date/time\n","               merged[\"__jst_dt\"] = merged.apply(lambda r: datetime.strptime(f\"{r['日付']} {r['開始時間'] or '00:00'}\", \"%Y/%m/%d %H:%M\"), axis=1)\n","               merged = merged.sort_values(\"__jst_dt\").drop(columns=\"__jst_dt\").reset_index(drop=True)\n","\n","        except gspread.exceptions.WorksheetNotFound:\n","            merged = new_df\n","            ws = sh.add_worksheet(title=sheet_name, rows=2000, cols=20)\n","\n","        # 指定の列順で書き込み\n","        final_columns = [\"日付\", \"開始時間\", \"終了時間\", \"動画時間\", \"タイトル\", \"種別\", \"URL\"]\n","        # データフレームに足りない列があれば空文字で埋める（念のため）\n","        for col in final_columns:\n","            if col not in merged.columns:\n","                merged[col] = \"\"\n","\n","        ws.clear()\n","        set_with_dataframe(ws, merged[final_columns])\n","\n","# -------------------------------\n","# (G) メイン処理\n","# -------------------------------\n","def main():\n","    channel_ids = get_id_list()\n","\n","    target_start_dt = None\n","    if TARGET_START_DATE:\n","        try:\n","            target_start_dt = datetime.strptime(TARGET_START_DATE, \"%Y/%m/%d\")\n","        except ValueError:\n","            print(f\"⚠️ TARGET_START_DATE の形式が不正です: {TARGET_START_DATE} (YYYY/MM/DD形式で指定してください)\")\n","\n","    for channel_id in channel_ids:\n","        print(f\"\\n=== 処理開始: {channel_id} ===\")\n","        try:\n","            uploads_id, channel_title = get_uploads_playlist_id(channel_id)\n","        except Exception as e:\n","            print(f\"  チャンネル情報取得失敗: {e}\")\n","            continue\n","\n","        # open or create spreadsheet for channel\n","        try:\n","            sh = gc.open(channel_title)\n","            is_new = False\n","        except gspread.SpreadsheetNotFound:\n","            sh = gc.create(channel_title)\n","            is_new = True\n","            if OPTIONAL_FOLDER_ID:\n","                try:\n","                  gc.request(\"post\", f\"/drive/v3/files/{sh.id}/parents\", json={\"addParents\": OPTIONAL_FOLDER_ID})\n","                except Exception:\n","                  pass\n","\n","        # determine since datetime (JST)\n","        start_jst = None\n","        if not is_new:\n","            start_jst = get_last_retrieved_datetime_for_sheet(sh)\n","\n","        effective_start_jst = start_jst\n","\n","        if target_start_dt:\n","            if start_jst is None or start_jst < target_start_dt:\n","                effective_start_jst = target_start_dt\n","                print(f\"  ★ 起点指定適用: {TARGET_START_DATE} 以降を確認します\")\n","\n","        if effective_start_jst is None:\n","            since_utc_iso = None\n","            print(\"  前回取得情報なし -> 全件取得\")\n","        else:\n","            # add one second to avoid duplicate\n","            next_jst = effective_start_jst + timedelta(seconds=1)\n","            utc_dt = next_jst - timedelta(hours=9)\n","            since_utc_iso = utc_dt.strftime(\"%Y-%m-%dT%H:%M:%SZ\")\n","            print(f\"  取得開始日時（JST）: {next_jst.strftime('%Y/%m/%d %H:%M')} -> (UTC): {since_utc_iso}\")\n","\n","        ids = get_video_ids_since(uploads_id, since_utc_iso)\n","\n","        if not ids:\n","            print(f\"=== 新規なし：{channel_title} ===\")\n","            continue\n","        print(f\"  取得候補動画数: {len(ids)}\")\n","        df_new = get_video_details(ids)\n","\n","        # merge and write to spreadsheet per month sheet\n","        append_and_merge_to_spreadsheet(sh, df_new)\n","        print(f\"=== 追加完了：{channel_title} ===\")\n","\n","        time.sleep(1)\n","    print(\"\\n全チャネル処理 完了\")\n","\n","if __name__ == \"__main__\":\n","    main()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"WThXEoFlnjm8","outputId":"354fe006-b371-4a52-974e-129d39b4fffa","collapsed":true,"executionInfo":{"status":"ok","timestamp":1769993436207,"user_tz":-540,"elapsed":69201,"user":{"displayName":"Hibiki Takegata","userId":"14263882105991529701"}}},"execution_count":1,"outputs":[{"output_type":"stream","name":"stdout","text":["Requirement already satisfied: google-api-python-client in /usr/local/lib/python3.12/dist-packages (2.188.0)\n","Requirement already satisfied: pandas in /usr/local/lib/python3.12/dist-packages (2.2.2)\n","Collecting pandas\n","  Downloading pandas-3.0.0-cp312-cp312-manylinux_2_24_x86_64.manylinux_2_28_x86_64.whl.metadata (79 kB)\n","\u001b[2K     \u001b[90m━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\u001b[0m \u001b[32m79.5/79.5 kB\u001b[0m \u001b[31m2.3 MB/s\u001b[0m eta \u001b[36m0:00:00\u001b[0m\n","\u001b[?25hRequirement already satisfied: gspread in /usr/local/lib/python3.12/dist-packages (6.2.1)\n","Requirement already satisfied: gspread_dataframe in /usr/local/lib/python3.12/dist-packages (4.0.0)\n","Collecting isodate\n","  Downloading isodate-0.7.2-py3-none-any.whl.metadata (11 kB)\n","Requirement already satisfied: httplib2<1.0.0,>=0.19.0 i